<componentContainer>
  <!--
    The helpLink element points to a page containing help text. This
    shows up as the target of the EC Help link in the top-right corner
    of the page. If you don't have page-specific help for this page, you
    should point this to the plugin help page.
  -->
  <helpLink>help</helpLink>

  <title>TestCGIPlugin @PLUGIN_VERSION@ helloworld Title</title>
  <table width="100%">
    <tr>
      <td valign="top" width="50%">
        <component plugin="EC-Core" ref="urlLoader">
          <plugin>TestCGIPlugin</plugin>
          <version>1.0.0.0</version>
          <url>cgi-bin/helloWorld.cgi</url>
        </component>
      </td>
    </tr>
  </table>
  <!-- Example of how to reference an image from the plugin's htdocs directory-->
  <img src="../../plugins/TestCGIPlugin-1.0.0.0/electric-cloud_logo-sm.gif"/>
</componentContainer>
